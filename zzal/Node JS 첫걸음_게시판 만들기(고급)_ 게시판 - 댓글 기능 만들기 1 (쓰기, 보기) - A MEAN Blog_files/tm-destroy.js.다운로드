var destroyTutorial = function(){
  if(window.TMS) window.TMS.inactivate();

  if(window.myWatch){
    if(window.myWatch.inactivate) window.myWatch.inactivate();
    delete window.myWatch;
  }
  if(window.myMovingObject){
    if(window.myMovingObject.inactivate) window.myMovingObject.inactivate();
    delete window.myMovingObject;
  }
  if(window.myMovingObject2){
    if(window.myMovingObject2.inactivate) window.myMovingObject2.inactivate();
    delete window.myMovingObject2;
  }
  if(window.myFrame){
    if(window.myFrame.inactivate) window.myFrame.inactivate();
    delete window.myFrame;
  }
  if(window.main){
    if(window.main.inactivate) window.main.inactivate();
    delete window.main;
  }
  if(window.myInputMgr){
    if(window.myInputMgr.inactivate) window.myInputMgr.inactivate();
    delete window.myInputMgr;
  }
  if(window.mainInterval){
    window.clearInterval(mainInterval);
    delete window.mainInterval;
  }
  if(window.frame){
    if(window.frame.inactivate) window.frame.inactivate();
    delete window.frame;
  }
  if(window.enemy){
    if(window.enemy.inactivate) window.enemy.inactivate();
    delete window.enemy;
  }

};
